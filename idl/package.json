{
  "name": "idl-vscode",
  "displayName": "ISA Description Language (IDL) Language Support",
  "description": "Language support for the Isa Description Language",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dhower-qc/idl-vscode",
    "directory": "idl"
  },
  "publisher": "HowerLimited",
  "contributes": {
    "languages": [{
      "id": "idl",
      "aliases": ["IDL", "idl"],
      "extensions": [".idl",".isa"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "idl",
      "scopeName": "source.idl",
      "path": "./syntaxes/idl.tmLanguage.json"
    }]
  }
}
